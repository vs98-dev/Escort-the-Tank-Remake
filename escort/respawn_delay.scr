respawn local.player: 
local.player = self

if (level.insane_obj == 1) 
{

if (self.dmteam != spectator)
{
wait 2

	self notsolid	
	self physics_off
	self hide

self stufftext "locationprint 260 200 Respawning-in...3...seconds!!"
self notsolid
self hide
wait 1
self stufftext "locationprint 260 200 Respawning-in...2...seconds!!"
self notsolid
self hide
wait 1
self stufftext "locationprint 260 200 Respawning-in...1...second!!"
self notsolid
self hide
wait 1

	self solid
	self physics_on
	
	self respawn
	self show
}
//self stufftext "locationprint 310 200 GO!!"
}



if ((level.allies_side == 1) && (level.axis_side == 0))
{

if (self.dmteam == allies)
{
wait 2

	self notsolid	
	self physics_off
	self hide

self stufftext "locationprint 260 200 Respawning-in...3...seconds!!"
