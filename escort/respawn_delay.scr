self notsolid
self hide
wait 1
self stufftext "locationprint 260 200 Respawning-in...1...second!!"
self notsolid
self hide
wait 1

	self solid
	self physics_on
	
	self respawn
	self show

//self stufftext "locationprint 310 200 GO!!"
}

}
else if ((level.allies_side == 0) && (level.axis_side == 1))
{

if (self.dmteam == axis)
{
wait 2

	self notsolid	
	self physics_off
	self hide

self stufftext "locationprint 260 200 Respawning-in...3...seconds!!"
self notsolid
self hide
wait 1
